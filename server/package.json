{
  "name": "lms-startup",
  "version": "1.0.0",
  "description": "```mermaid",
  "type": "module",
  "main": "./api/index.js",
  "scripts": {
    "pretty": "npx prettier . --write",
    "dev": "NODE_ENV=dev tsx watch ./src/app.ts",
    "prod": "NODE_ENV=prod tsx ./src/app.ts",
    "start": "NODE_ENV=prod tsx ./src/app.ts",
    "start:dist": "NODE_ENV=prod node ./dist/src/app.js",
    "vercel-build": "echo hello",
    "test": "NODE_ENV=test jest ./src/__tests__/ --config jest.config.js --forceExit --verbose --runInBand --coverage --detectOpenHandles",
    "lint": "npx eslint ./src",
    "generate": "graphql-codegen --config ./codegen.config.ts",
    "magidoc:auth": "CONFIG=auth magidoc generate",
    "magidoc:user": "CONFIG=user magidoc generate",
    "magidoc:admin": "CONFIG=admin magidoc generate",
    "magidoc:token": "CONFIG=tokenManager magidoc generate",
    "magidoc:all": "npm run magidoc:auth && npm run magidoc:user && npm run magidoc:admin && npm run magidoc:token"
  },
  "author": "",
  "license": "ISC",
